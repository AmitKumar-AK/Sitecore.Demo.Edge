{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "OpenAQAirSearch": {
    "OpenAQAirEndPoint": "https://api.openaq.org/v2",
    "DefaultPageSize": 10
  },
  "Serilog": {
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "Logs\\SitecoreDemoEdgeAPI-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
